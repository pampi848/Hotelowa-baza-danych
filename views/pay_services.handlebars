<div class="page-header">
   <h1>Bought services by {{item}}</h1>
</div>
{{#if list}}
    <table class="table table-hover">
        <thead>
            <tr class="row">
                    <th>id</th>
                    <th>name</th>
                    <th>price</th>
                    <th>description</th>
                    <th>created</th>
                    <th>id_client</th>
                    <th>id_service</th>
                    <th>paid</th>
            </tr>
        </thead>
        {{#each list}}
            <tbody>
                <tr class="row">
                    {{#each this}}
                        <td>{{this}}</td>
                    {{/each}}
                </tr>
            </tbody>
        {{/each}}
    </table>
    <div class="row">
        <div  class="col-sm-5"></div>
        <div  class="col-sm-7">
            <h2>Sum: {{sum}}</h2>
        </div>
    </div>
    {{else}}
    No services are bought.
{{/if}}